<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
         <css src="Humcommerce_LegalPages::css/vue-wizard.css"/>
         <css src="Humcommerce_LegalPages::css/select2.css"/>

    </head>
    <referenceBlock name="page.title">
        <action method="setPageTitle">
            <argument name="title" xsi:type="string">Wizard</argument>
        </action>
    </referenceBlock>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Backend\Block\Template" template="Humcommerce_LegalPages::wizard.phtml"/>
        </referenceContainer>
    </body>
</page>